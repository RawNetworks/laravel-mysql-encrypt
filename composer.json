{
    "name": "chr15k/laravel-mysql-encrypt",
    "description": "Laravel database encryption using native MySQL functions",
    "keywords": [
        "laravel",
        "chr15k",
        "mysql",
        "encryption",
        "php",
        "pii",
        "gdpr"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Christopher Keller",
            "email": "ctk8501@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/console": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/contracts": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.5",
        "phpunit/phpunit": "^9.6|^10.5|^11.4"
    },
    "autoload": {
        "psr-4": {
            "Chr15k\\MysqlEncrypt\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Chr15k\\MysqlEncrypt\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chr15k\\MysqlEncrypt\\Providers\\MysqlEncryptServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
